<Application
    x:Class="MyoUWP.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MyoUWP"
    RequestedTheme="Light">

    <Application.Resources>
        <ImageBrush x:Key="myoBrush" ImageSource="Assets/myo.png" Stretch="Fill" Opacity="0.75" />
        <ImageBrush x:Key="myoScoresBrush" ImageSource="Assets/myo.png" Stretch="Fill" Opacity="0.85" />
        <ImageBrush x:Key="marsBrush" ImageSource="Assets/mars.jpg" Stretch="Fill" Opacity="0.75" />
    </Application.Resources>

</Application>
